    <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
  </div>
  <div class="carousel-inner">
    <div class="carousel-item active" style="max-height: 500px">
      <img src="http://www.orleikantorjr.com.br/wp-content/uploads/2019/11/CLINICA-DE-DOEN%C3%87AS-RESPIRAT%C3%93RIAS-DA-INF%C3%82NCIA-E-ADOLESC%C3%8ANCIA-4.png" class="d-block w-100" alt="...">
